            application/json: {}
  /api/_search/users:
    get:
      tags:
        - Mobile APIs
      summary: Get Sample Using Search
      security:
        - bearerAuth: []
      parameters:
        - name: page
          in: query
          schema:
            type: integer
          example: '0'
        - name: search
          in: query
          schema:
            type: integer
          example: '46'
        - name: size
          in: query
          schema:
            type: integer
          example: '20'
        - name: sort
          in: query
          schema:
            type: string
          example: id,asc
      responses:
        '200':
          description: Successful response
          content:
            application/json: {}
  /api/invoices/filter:
    get:
      tags:
        - Mobile APIs
      summary: Get Invoice By Filter
      security:
        - bearerAuth: []
      parameters:
        - name: amount_lower
          in: query
          schema:
            type: integer
          example: '0'
        - name: balance_lower
          in: query
          schema:
            type: integer
          example: '0'
        - name: only_return_invoices
          in: query
          schema:
            type: boolean
          example: 'true'
        - name: page
          in: query
          schema:
            type: integer
          example: '0'
        - name: paid
          in: query
          schema:
            type: boolean
          example: 'false'
        - name: size
          in: query
          schema:
            type: integer
          example: '20'
        - name: sort
          in: query
          schema:
            type: string
          example: invoice_date,desc
        - name: status
          in: query
          schema:
            type: string
          example: APPROVED
      responses:
        '200':
          description: Successful response
          content:
            application/json: {}
  /api/authenticate:
    post:
      tags:
        - Mobile APIs
      summary: Authenticate User
      requestBody:
        content:
          application/json:
            schema:
              type: object
              example:
                password: Admin@123
                username: dishan.bhardwaj@technostacks.in
      responses:
        '200':
          description: Successful response
          content:
            application/json: {}