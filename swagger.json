        '200':
          description: Successful response
          content:
            application/json: {}
  /api/customer-orders:
    get:
      tags:
        - Mobile APIs
      summary: List Customer-Orders
      security:
        - bearerAuth: []
      parameters:
        - name: size
          in: query
          schema:
            type: integer
          example: '5'
        - name: page
          in: query
          schema:
            type: integer
          example: '0'
        - name: sort
          in: query
          schema:
            type: string
          example: dateCreated,desc
      responses:
        '200':
          description: Successful response
          content:
            application/json: {}
  /api/invoices/attachment/1071:
    get:
      tags:
        - Mobile APIs
      summary: Get Invoice Attachment by ID
      security:
        - bearerAuth: []
      responses:
        '200':
          description: Successful response
          content:
            application/json: {}
  /api/payment/filter:
    get:
      tags:
        - Mobile APIs
      summary: Payment-Filter
      parameters:
        - name: size
          in: query
          schema:
            type: integer
          example: '15'
        - name: page
          in: query
          schema:
            type: integer
          example: '0'
        - name: sort
          in: query
          schema:
            type: string
          example: id%2Cdesc
        - name: paid
          in: query
          schema:
            type: string
          example: 'false'' '
      responses:
        '200':
          description: Successful response
          content:
            application/json: {}
  /api/panels:
    get:
      tags:
