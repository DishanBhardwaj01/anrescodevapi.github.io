openapi: 3.0.0
info:
  title: Anresco
  version: 1.0.0
servers:
  - url: http://localhost:9000
  - url: https://anresco-portal-dev2.qbench.net
components:
  securitySchemes:
    bearerAuth:
      type: http
      scheme: bearer
tags:
  - name: Local
  - name: Dev
paths:
  /api/_search/customer-orders:
    get:
      tags:
        - Mobile APIs
      summary: Order-List (DEV)
      security:
        - bearerAuth: []
      parameters:
        - name: page
          in: query
          schema:
            type: integer
          example: '0'
        - name: query
          in: query
          schema:
            type: integer
          example: '6183'
        - name: size
          in: query
          schema:
            type: integer
          example: '20'
        - name: sort
          in: query
          schema:
            type: string
          example: dateReceived,desc
      responses:
        '200':
          description: Successful response
          content:
            application/json: {}
  /api/samples/mobile-lot:
    get:
      tags:
        - Mobile APIs
      summary: Mobile-Lot (DEV)
